[github]

# minimal settings
tpope vim-sensible

# vimscript, shell, etc. auto-closer
tpope vim-endwise

# autodetect tab width
tpope vim-sleuth

# syntax checker
scrooloose syntastic

# indent highlighter
Yggdroot indentLine
